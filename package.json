{
  "name": "root_child_composition_sample",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap-cloud-sdk/http-client": "^3.20.0",
    "@sap-cloud-sdk/resilience": "^3.20.0",
    "@sap/cds": "^8",
    "express": "^4",
    "generator-easy-ui5": "^3.8.1",
    "yo": "^5.0.0"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.6",
    "@cap-js/sqlite": "^1",
    "@sap/ux-specification": "^1.124.0",
    "cds-plugin-ui5": "^0.9.3"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-fe_rootchild_sample": "cds watch --open fe_rootchild_sample/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-fe_rootchild_flexible": "cds watch --open fe_rootchild_flexible/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-fpm_rootchild": "cds watch --open fpmrootchild/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "cds": {
    "requires": {
      "zui_rap_capability_test_anno": {
        "kind": "odata",
        "model": "srv/external/zui_rap_capability_test_anno",
        "credentials": {
          "destination": "A4H_OnPremise",
          "path": "/sap/opu/odata4/sap/zui_rap_capability_test_anno/srvd/sap/zui_rap_capability_test_anno/0001"
        }
      }
    }
  },
  "sapux": [
    "app/fe_rootchild_sample",
    "app/fe_rootchild_flexible",
    "app/fpm_rootchild"
  ],
  "workspaces": [
    "app/*"
  ]
}
